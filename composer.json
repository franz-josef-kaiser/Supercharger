{
	"name": "wcm/chassis-supercharger",
	"description": "Supercharger for Chassis WordPress Vagrant box",
	"version": "0.1.0",
	"type": "library",
	"keywords": [
		"WordPress",
		"Vagrant",
		"Puppet",
		"wp-content"
	],
	"homepage": "http://github.com/franz-josef-kaiser/Supercharger",
	"time": "2013-04-23",
	"license": "MIT",
	"authors": [
		{
			"name": "Franz Josef Kaiser",
			"email": "wecodemore@gmail.com",
			"homepage": "http://unserkaiser.com",
			"role": "Developer"
		}
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"target-dir": "plugins",
	"require": {
		"wcm/wcm-lang-switch": "@dev",
		"regenerate-thumbnails": "@dev",
		"dynamic-image-resize": "@dev",
		"Rarst/fragment-cache": "@dev",
		"Rarst/laps": "@dev",
		"current-admin-info": "@dev"
	},
	"repositories": [
		{
			"packagist" : false
		},
		{
			"type": "package",
			"package": {
				"name": "wcm/wcm-lang-switch",
				"version": "@dev",
				"dist": {
					"url": "https://github.com/wecodemore/wcm_lang_switch/archive/master.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "regenerate-thumbnails",
				"version": "@dev",
				"dist": {
					"url": "https://github.com/Viper007Bond/regenerate-thumbnails.git",
					"type": "git"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "dynamic-image-resize",
				"version": "@dev",
				"dist": {
					"url": "https://github.com/franz-josef-kaiser/Dynamic-Image-Resize/archive/master.zip",
					"type": "git"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "Rarst/fragment-cache",
				"version": "@dev",
				"dist": {
					"url": "https://github.com/Rarst/fragment-cache/archive/master.zip",
					"type": "git"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "Rarst/laps",
				"version": "@dev",
				"dist": {
					"url": "https://bitbucket.org/Rarst/laps/get/ca5335d9b06d.zip",
					"type": "git"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "current-admin-info",
				"version": "@dev",
				"dist": {
					"url": "https://github.com/franz-josef-kaiser/current-admin-info/archive/master.zip",
					"type": "git"
				}
			}
		}
	]
}